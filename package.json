{
  "name": "evme-admin",
  "version": "0.2.1",
  "private": true,
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:stats": "npm run build -- --stats && webpack-bundle-analyzer ./build/bundle-stats.json",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --watchAll=false --runInBand",
    "test:e2e": "cypress run",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "eslint \"{src,cypress}/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:css": "stylelint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:css:fix": "npm run lint:css -- --fix",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "generate:types": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@date-io/dayjs": "~1.3.13",
    "@material-ui/core": "~4.12.2",
    "@material-ui/data-grid": "~4.0.0-alpha.34",
    "@material-ui/icons": "~4.11.2",
    "@material-ui/lab": "~4.0.0-alpha.60",
    "@material-ui/pickers": "~3.3.10",
    "@react-google-maps/api": "~2.2.0",
    "@sentry/browser": "~6.4.1",
    "@sentry/tracing": "~6.4.1",
    "@tinymce/tinymce-react": "~3.13.0",
    "@types/jwt-decode": "~3.1.0",
    "@types/qs": "~6.9.7",
    "axios": "~0.21.1",
    "dayjs": "~1.10.5",
    "diff": "~5.0.0",
    "firebase": "~8.7.0",
    "formik": "~2.2.8",
    "graphql-request": "~3.4.0",
    "i18next": "~20.3.1",
    "i18next-browser-languagedetector": "~6.1.1",
    "jwt-decode": "~3.1.2",
    "localstorage-slim": "~1.9.8",
    "lodash": "~4.17.21",
    "react": "^17.0.2",
    "react-beforeunload": "~2.5.3",
    "react-device-detect": "~2.2.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "~1.0.2",
    "react-i18next": "~11.10.0",
    "react-query": "~3.16.0",
    "react-router-dom": "~5.2.0",
    "react-scripts": "4.0.3",
    "styled-components": "~5.3.0",
    "web-vitals": "^1.0.1",
    "yup": "~0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "~12.1.4",
    "@commitlint/config-conventional": "~12.1.4",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "~1.22.1",
    "@graphql-codegen/typescript-operations": "1.18.0",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/dom": "^7.31.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^5.1.2",
    "@testing-library/user-event": "^12.8.3",
    "@types/css-mediaquery": "~0.1.1",
    "@types/diff": "~5.0.1",
    "@types/googlemaps": "~3.43.3",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/query-string": "~6.3.0",
    "@types/react": "^17.0.0",
    "@types/react-beforeunload": "~2.1.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "~5.1.7",
    "@types/styled-components": "~5.1.9",
    "css-mediaquery": "~0.1.2",
    "cypress": "~7.3.0",
    "deepmerge": "~4.2.2",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-chai-friendly": "~0.7.1",
    "eslint-plugin-cypress": "~2.11.3",
    "eslint-plugin-filenames": "~1.3.2",
    "eslint-plugin-jest": "~24.3.6",
    "eslint-plugin-jsx-a11y": "~6.4.1",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-react": "~7.23.2",
    "eslint-plugin-react-hooks": "~4.2.0",
    "eslint-plugin-testing-library": "~4.6.0",
    "husky": "~6.0.0",
    "jest-styled-components": "~7.0.4",
    "lint-staged": "~11.0.0",
    "msw": "~0.29.0",
    "prettier": "~2.3.0",
    "stylelint": "~13.13.1",
    "stylelint-a11y": "~1.2.3",
    "stylelint-config-prettier": "~8.0.2",
    "stylelint-config-standard": "~22.0.0",
    "stylelint-group-selectors": "~1.0.8",
    "stylelint-prettier": "~1.2.0",
    "stylelint-use-nesting": "~3.0.0",
    "typescript": "~4.2.4",
    "webpack-bundle-analyzer": "~4.4.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!**/tests/*",
      "!src/config.ts",
      "!src/index.tsx",
      "!src/setupTests.ts",
      "!src/setupProxy.ts",
      "!src/serviceWorker.ts",
      "!src/react-app-env.d.ts",
      "!src/reportWebVitals.ts"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
